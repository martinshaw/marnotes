{
    "name": "marnotes-web",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "esbuild public/typescript/app.tsx --bundle --minify --format=iife --outfile=public/dist/app.js --jsx-factory=React.createElement --jsx-fragment=React.Fragment",
        "build:dev": "esbuild public/typescript/app.tsx --bundle --sourcemap --format=iife --outfile=public/dist/app.js --jsx-factory=React.createElement --jsx-fragment=React.Fragment"
    },
    "devDependencies": {
        "esbuild": "^0.27.2",
        "typescript": "^5.3.3"
    }
}
